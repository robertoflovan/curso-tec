.Contenido
	background green
	width 100%
	display flex
	flex-direction column
	overflow-y scroll
	&-skill
		background-image url(../img/1.jpg)
		background-size cover
		min-height 14em 
		display flex
		&-info
			display flex
			flex-direction column
			justify-content center
			padding-left 28em
			align-items flex-end
			padding-right 4em
			width 100%;
			background: rgba(188,224,238,0.33);
			background: -moz-linear-gradient(left, rgba(188,224,238,0.33) 0%, rgba(41,184,229,0.67) 50%, rgba(179,220,237,1) 100%);
			background: -webkit-gradient(left top, right top, color-stop(0%, rgba(188,224,238,0.33)), color-stop(50%, rgba(41,184,229,0.67)), color-stop(100%, rgba(179,220,237,1)));
			background: -webkit-linear-gradient(left, rgba(188,224,238,0.33) 0%, rgba(41,184,229,0.67) 50%, rgba(179,220,237,1) 100%);
			background: -o-linear-gradient(left, rgba(188,224,238,0.33) 0%, rgba(41,184,229,0.67) 50%, rgba(179,220,237,1) 100%);
			background: -ms-linear-gradient(left, rgba(188,224,238,0.33) 0%, rgba(41,184,229,0.67) 50%, rgba(179,220,237,1) 100%);
			background: linear-gradient(to right, rgba(188,224,238,0.33) 0%, rgba(41,184,229,0.67) 50%, rgba(179,220,237,1) 100%);
			


@media(max-width:680px)
	.Contenido
		flex-direction row
		flex-wrap wrap
		&-skill
			width 50%
			&-info
				padding 1em;